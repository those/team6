<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobshep.template"
    android:versionCode="1"
    android:versionName="1.0" >

    <permission
        android:protectionLevel="dangerous"
        android:icon="@drawable/ic_launcher"
        android:name="com.mobshep.template.START_ACTIVITY"
        android:logo="@drawable/ic_launcher">
    </permission>


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/Theme.AppCompat"
        android:label="@string/app_name">

        <activity
            android:name="com.mobshep.template.template"
            android:label="@string/app_name"
            android:permission="com.mobshep.template.START_ACTIVITY">
            <intent-filter>
                <action android:name="com.mobshep.template.template" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.mobshep.template.Splash"
            android:label="@string/app_name"
            android:permission="com.mobshep.template.START_ACTIVITY" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity
            android:name="com.mobshep.template.Preferences"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.mobshep.template.Preferences" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


    </application>

</manifest>