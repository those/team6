<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobshep.poorauthentication1" >

    <permission
        android:name="com.mobshep.PA1.START_ACTIVITY"
        android:icon="@drawable/ic_launcher"
        android:logo="@drawable/ic_launcher"
        android:protectionLevel="dangerous" >
    </permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoggedIn"
            android:permission="com.mobshep.PA1.START_ACTIVITY"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.mobshep.poorauthentication1.LoggedIn" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>
